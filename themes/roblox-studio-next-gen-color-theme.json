	"tokenColors": [
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#BCBEC8"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#F2BA2A"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#8EE9B6"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#6A6F81"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
			],
			"settings": {
				"foreground": "#EB7973"
			}
		},
		//TODO: Review these?
		{
			"name": "Error",
			"scope": [
				"invalid",
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#DF281F"
			}
		},
		{
			"name": "Warning",
			"scope": [
				"warning"
			],
			"settings": {
				"foreground": "#F57630"
			}
		},
		{
			"name": "Information",
			"scope": [
				"info"
			],
			"settings": {
				"foreground": "#494D5A"
			}
		},
		{
			"name": "Hint",
			"scope": [
				"hint"
			],
			"settings": {
				"foreground": "#335FFF"
			}
		},
		//---------------------------------
		{
			"name": "Built-in Function",
			"scope": [
				"constant.language",
				"support.function"
			],
			"settings": {
				"foreground": "#8FB4FF"
			}
		},
		{
			"name": "Local Method Color",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#FAE4AA"
			}
		},
		{
			"name": "Local Property Color",
			"scope": [
				"variable.other.property"
			],
			"settings": {
				"foreground": "#70A0FF"
			}
		},
		{
			"name": "nil",
			"scope": [
				"constant.language.nil"
			],
			"settings": {
				"foreground": "#F2BA2A"
			}
		},
		{
			"name": "Bool",
			"scope": [
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#F2BA2A"
			}
		},
		{
			"name": "control", //function, if, else, return, then
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#EB7973"
			}
		},
		{
			"name": "local",
			"scope": [
				"storage.modifier.local"
			],
			"settings": {
				"foreground": "#EB7973"
			}
		},
		{
			"name": "self",
			"scope": [
				"variable.language.self"
			],
			"settings": {
				"foreground": "#EB7973"
			}
		},
		{
			"name": "not", //This isnt in the studio settings
			"scope": [
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#EB7973"
			}
		},
		{
			"name": "export, type", //This isnt in the studio settings
			"scope": [
				"storage.modifier.visibility",
				"storage.type"
			],
			"settings": {
				"foreground": "#EB7973"
			}
		},
		{
			"name": "Function name", //Overlaps with Local Method
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#FAE4AA"
			}
		},
		// {
		// 	"name": "TODO",
